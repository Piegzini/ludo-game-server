{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": [
      "warn"
    ],
    "no-restricted-syntax": [
      "off"
    ],
    "no-underscore-dangle": [
      "off"
    ],
    "no-return-await": [
      "off"
    ]
  }
}
